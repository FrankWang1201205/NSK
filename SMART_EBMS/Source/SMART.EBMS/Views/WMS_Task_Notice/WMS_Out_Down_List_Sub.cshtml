@model List<WMS_Out_Head>
@{
    Layout = null;
}

<style>
     * {
        box-sizing: border-box;
    }
    table {
        border-spacing: 0;
        border-collapse: collapse;
        
    }
    table tr td{
        padding-left:20px;
        text-align:left;
        font-size: 15px;
        border-top:0;
    }  
    table tr:nth-child(even) {
        background: whitesmoke;
    }
</style>


<table class="table text-nowrap table-hover" style="width:100%;font-size:15px;">

    <tbody>
        @foreach (var x in Model)
        {
            <tr>
                <td style="vertical-align:middle;width:150px;">@x.Task_Bat_No_Str</td>
                <td style="vertical-align:middle;width:160px;">@x.MatSn_Count.ToString("N0")</td>
                <td style="vertical-align:middle;width:160px;">@x.Quantity_Sum.ToString("N0")</td>
                <td style="vertical-align:middle;width:130px;">@x.Work_Down_Person</td>
            </tr>
        }
    </tbody>
</table>
